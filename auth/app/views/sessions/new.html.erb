<h1>Log in</h1>
<%= link_to 'Sign in with Google', '/auth/google_oauth2' %>
<%= form_with(scope: :session, url: sessions_path) do |f| %>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.email_field :email %>
  </div>

  <div class="field">
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>

  <div class="actions">
    <%= f.submit 'Log in' %>
  </div>
<% end %>